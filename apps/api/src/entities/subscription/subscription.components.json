{
  "schemas": {
    "Subscription": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "refreshRate": {
          "type": "integer"
        },
        "subscriptionType": {
          "type": "string"
        },
        "price": {
          "type": "integer"
        },
        "level": {
          "type": "integer"
        },
        "hidden": {
          "type": "boolean"
        },
        "isBestValue": {
          "type": "boolean"
        },
        "hasFreeTrial": {
          "type": "boolean"
        },
        "freeTrialDays": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        },
        "paymentProvider": {
          "type": "string",
          "enum": [
            "stripe",
            "payline"
          ]
        },
        "subDesc": {
          "type": "string"
        },
        "limited": {
          "type": "number"
        }
      },
      "required": [
        "name",
        "description",
        "subscriptionType",
        "price",
        "level"
      ]
    }
  }
}
